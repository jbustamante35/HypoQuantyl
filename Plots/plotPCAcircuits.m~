function plotPCAcircuits(X, Y, c, n, f)
%% plotPCAcircuits: plot to check PCA results of fully normalized manual contours
%
%
% Usage:
%
%
% Input:
%   X: output from my custom pca analysis for x-coordinates
%   Y: output from my custom pca analysis for y-coordinates
%   c: array of CircuitJB objects used for pca analysis
%   n: index of CircuitJB in c to plot data
%   f: 0 to overlay plot on current figure, 1 to create new figure
%
% Output:
%
%

%% Set-up new figure or replace figure, and set-up function handle for contour conversion
if f
    figure;
else
    cla;
    clf;
end

set(gcf, 'Color', 'w');
n2i = @(x,y) norm2interp(y, x.getMean, x.getAnchors(1, 'b2'), x.getAnchors(1, 'd2'));

%%
raw = [X.InputData(n,:) ; Y.InputData(n,:)]';
trc = [X.SimData(n,:) ; Y.SimData(n,:)]';
rts = c(n).getRoute;

subplot(211);
hold on;

subplot(212);
imagesc(c(n).getImage(1, 'gray'));
colormap gray, axis image;
hold on;

%%
j = 1;
szA = size(rts,2);
szB = size(trc,1);
szC = szB / szA;
% for k = (szB/szA) : (szB/szA) : szB
%     C = rand(1,3);
%     
%     subplot(211);
%     plot(X.InputData(n, (k-299:k)), Y.InputData(n,(k-299:k)), 'o-', 'Color', C);
%     plot(X.SimData(n,(k-299:k)), Y.SimData(n,(k-299:k)), '--', 'Color', C);
%     title(sprintf('Input vs Synthetic: Normalized Contours %d', n));
%     
%     subplot(212);
%     R = n2i(rts(j), raw((k-299): k, :));
%     I = n2i(rts(j), trc((k-299): k, :));
%     plot(R(:,1), R(:,2), 'x', 'MarkerSize', 5, 'Color', C);
%     plot(I(:,1), I(:,2), '.', 'MarkerSize', 5, 'Color', C);
%     title(sprintf('Input vs Synthetic: Overlay on Image %d', n));
%     j = j + 1;
% end

for k = (szB/szA) : (szB/szA) : szB
    C = rand(1,3);
    
    subplot(211);
    plot(X.InputData(n, (k - (szB-1) : k)), Y.InputData(n, (k - (szB-1) : k)), 'o-', 'Color', C);
    plot(X.SimData(n, (k - (szB-1) : k)), Y.SimData(n, (k - (szB-1) : k)), '--', 'Color', C);
    title(sprintf('Input vs Synthetic: Normalized Contours %d', n));
    
    subplot(212);
    R = n2i(rts(j), raw((k - (szB - 1)) : k, :));
    I = n2i(rts(j), trc((k - (szB - 1)) : k, :));
    plot(R(:,1), R(:,2), 'x', 'MarkerSize', 5, 'Color', C);
    plot(I(:,1), I(:,2), '.', 'MarkerSize', 5, 'Color', C);
    title(sprintf('Input vs Synthetic: Overlay on Image %d', n));
    j = j + 1;
end


end